{
  "name": "genome-system-contracts",
  "version": "0.0.0",
  "description": "EWF's Genome chain system contracts.",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/energywebfoundation/genome-system-contracts.git"
  },
  "keywords": [
    "ewf",
    "energyweb",
    "system",
    "contracts",
    "genome"
  ],
  "author": "Adam Nagy <adam.nagy@energyweb.org>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/energywebfoundation/genome-system-contracts/issues"
  },
  "homepage": "https://github.com/energywebfoundation/genome-system-contracts#readme",
  "scripts": {
    "test": "scripts/test.sh",
    "compile": "npx truffle compile",
    "migrate": "npx truffle migrate",
    "coverage": "npx solidity-coverage",
    "coverage_publish": "scripts/coveralls.sh",
    "lint:solium": "npx solium -d contracts/",
    "lint:solium:fix": "npx solium -d contracts/ --fix",
    "lint:solhint": "npx solhint \"contracts/**/*.sol\"",
    "ganache": "npx ganache-cli --port 8545",
    "parity": "parity --chain dev --jsonrpc-port 8545 --jsonrpc-apis all",
    "security": "slither . --config-file .slither.conf.json"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.1.1",
    "coveralls": "^3.0.3",
    "ethlint": "^1.2.3",
    "ganache-cli": "^6.3.0",
    "solhint": "1.5.1",
    "solidity-coverage": "^0.6.0-beta.3",
    "solium-plugin-zeppelin": "0.0.2",
    "truffle": "^5.0.5",
    "web3": "^1.0.0-beta.37"
  },
  "dependencies": {}
}
